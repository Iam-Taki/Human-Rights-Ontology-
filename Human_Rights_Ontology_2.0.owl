    <!-- http://example.org/ontology#Article1 -->

    <NamedIndividual rdf:about="http://example.org/ontology#Article1">
        <rdf:type rdf:resource="http://example.org/ontology#HumanRights"/>
        <ex:articleNumber rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">1</ex:articleNumber>
        <ex:description>All human beings are born free and equal in dignity and rights.</ex:description>
    </NamedIndividual>
    


    <!-- http://example.org/ontology#FreedomOfSpeechInstance -->

    <NamedIndividual rdf:about="http://example.org/ontology#FreedomOfSpeechInstance">
        <rdf:type rdf:resource="http://example.org/ontology#LibertyOfExpression"/>
        <ex:articleNumber rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">19</ex:articleNumber>
        <ex:description>The right to express opinions without censorship or restraint.</ex:description>
        <ex:examples>Free press, public debates.</ex:examples>
        <ex:legalDocuments>ICCPR, First Amendment to the U.S. Constitution.</ex:legalDocuments>
        <ex:origin>Enlightenment principles of free thought.</ex:origin>
    </NamedIndividual>
    


    <!-- http://example.org/ontology#RightToCleanEnvironmentInstance -->

    <NamedIndividual rdf:about="http://example.org/ontology#RightToCleanEnvironmentInstance">
        <rdf:type rdf:resource="http://example.org/ontology#RightToCleanEnvironment"/>
        <ex:articleNumber rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">30</ex:articleNumber>
        <ex:description>The right to live in a clean and healthy environment.</ex:description>
        <ex:examples>Clean air, clean water.</ex:examples>
        <ex:legalDocuments>Various environmental protection laws.</ex:legalDocuments>
        <ex:origin>Environmental justice movements.</ex:origin>
    </NamedIndividual>
    


    <!-- http://example.org/ontology#RightToEducationInstance -->

    <NamedIndividual rdf:about="http://example.org/ontology#RightToEducationInstance">
        <rdf:type rdf:resource="http://example.org/ontology#AccessToEducation"/>
        <ex:articleNumber rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">26</ex:articleNumber>
        <ex:description>The right to access education.</ex:description>
        <ex:examples>Universal primary education.</ex:examples>
        <ex:legalDocuments>Convention on the Rights of the Child (CRC).</ex:legalDocuments>
        <ex:origin>Enlightenment and progressive education movements.</ex:origin>
    </NamedIndividual>
    


    <!-- http://example.org/ontology#RightToFairTrialInstance -->

    <NamedIndividual rdf:about="http://example.org/ontology#RightToFairTrialInstance">
        <rdf:type rdf:resource="http://example.org/ontology#ImpartialJustice"/>
        <ex:articleNumber rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">10</ex:articleNumber>
        <ex:description>The right to a fair and public hearing by an independent and impartial tribunal.</ex:description>
        <ex:examples>Right to legal representation, presumption of innocence.</ex:examples>
        <ex:legalDocuments>ICCPR, European Convention on Human Rights.</ex:legalDocuments>
        <ex:origin>Historical struggles for justice.</ex:origin>
    </NamedIndividual>
    


    <!-- http://example.org/ontology#RightToHealthcareInstance -->

    <NamedIndividual rdf:about="http://example.org/ontology#RightToHealthcareInstance">
        <rdf:type rdf:resource="http://example.org/ontology#AccessToHealthcare"/>
        <ex:articleNumber rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">25</ex:articleNumber>
        <ex:description>The right to access healthcare services.</ex:description>
